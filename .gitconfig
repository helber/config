[color]
    ui = auto
[color "branch"]
    current = yellow reverse
    local = yellow
    remote = green
[color "diff"]
    meta = yellow bold
    frag = magenta bold
    old = red bold
    new = green bold
[color "status"]
    added = yellow
    changed = green
    untracked = cyan
[color "diff"]
    whitespace = red reverse
[core]
    whitespace=fix,-indent-with-non-tab,trailing-space,cr-at-eol
[user]
    name = Helber Maciel Guerra
    editor = vim
    email = helber.guerra@ahgora.com.br
[alias]
    s = status
    b = branch -v
    co = checkout
    l = log
    d = diff
    c = commit -v
    graph = log --graph --abbrev-commit --date=relative --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(blue)<%an>%Creset'
    sumario = log --pretty=format:'* %Cred%h%Creset [%ad] %C(yellow)%s%Creset'
    tagl = log --date-order --graph --tags --simplify-by-decoration --pretty=format:'%ai %h %d'
[push]
    default = matching
[gitreview]
    username = helber
[gerrit "default"]
    host = review.openstack.org
    port = 29418
    user = helber
